# ----------------------------------------------------------------------------------------------
# Git Ignores @see https://github.com/github/gitignore
# ----------------------------------------------------------------------------------------------
!.gitignore

# Application Files
# ----------------------------------------------------------------------------------------------

# Ignore sensitive files
*.env
*.env.*
!*.env.example

# Ignore directories generated by Composer
vendor/*
!vendor/.gitkeep


# Wordpress Files
# ----------------------------------------------------------------------------------------------

# WordPress & Core Files
!web/
web/*
!web/index.php
!web/wp-cli.yml
!web/wp-config.php
!web/robots.txt
!web/favicon.ico
!web/.htaccess
!web/9asu71n2faky32gz4pa345svvw6n47.html
web/wp-config-local.php
web/wp-cli.local.yml
web/wp
!web/wp-content/
web/wp-content/*
web/wp-content/blogs.dir/
web/wp-content/upgrade/
web/wp-content/backup-db/
web/wp-content/updraft/
web/wp-content/backupwordpress-*/
web/wp-content/managewp/backups/
web/wp-content/advanced-cache.php
web/wp-content/wp-cache-config.php
web/sitemap.xml
web/sitemap.xml.gz
web/wp-content/cache/
web/wp-content/backups/

# WordPress wp-content
!web/wp-content/object-cache.php

# WordPress uploads
!web/wp-content/uploads/
web/wp-content/uploads/*
!web/wp-content/uploads/.gitkeep

# WordPress plugins
!web/wp-content/plugins/
web/wp-content/plugins/*
!web/wp-content/plugins/index.php
!web/wp-content/plugins/.gitkeep
!web/wp-content/plugins/real-media-library

# WordPress mu-plugins
!web/wp-content/mu-plugins/
web/wp-content/mu-plugins/*
!web/wp-content/mu-plugins/index.php
!web/wp-content/mu-plugins/.gitkeep
!web/wp-content/mu-plugins/bedrock-autoloader.php
!web/wp-content/mu-plugins/disallow-indexing.php
!web/wp-content/mu-plugins/register-theme-directory.php
!web/wp-content/mu-plugins/pantheon/
!web/wp-content/mu-plugins/pantheon.php
!web/wp-content/mu-plugins/disable-htaccess-rewrite-rules.php

# WordPress themes
!web/wp-content/themes/
!web/wp-content/themes/index.php
!web/wp-content/themes/.gitkeep


# Custom Development for Wordpress Files
# Add Wordpress plugins that require files to be checked into the repo
# ----------------------------------------------------------------------------------------------
!web/wp-content/plugins/wp-migrate-db-pro
!web/wp-content/plugins/wp-migrate-db-pro-cli
!web/wp-content/plugins/wp-migrate-db-pro-media-files
!web/wp-content/mu-plugins/wp-migrate-db-pro-compatibility.php
!web/wp-content/mu-plugins/disable-wp-admin.php
!web/wp-content/mu-plugins/disable-plugins-for-development.php


# Pantheon
# ----------------------------------------------------------------------------------------------
pantheon.upstream.yml

# Pantheon Quicksilver Scripts
!web/private/
web/private/*
!web/private/scripts/
web/private/scripts/quicksilver


# Bower / Node / Grunt / SASS
# @see - http://stackoverflow.com/questions/39990017/should-i-commit-the-yarn-lock-file-and-what-is-it-for
# ----------------------------------------------------------------------------------------------
*node_modules
*bower_components
*.grunt
*.sass-cache
*.tmp
*tmp
*.seed
*.csv
*.dat
*.out
*.pid
*.swp


# Vagrant
# ----------------------------------------------------------------------------------------------
.vagrant
vagrant.local.yml


# OSX
# ----------------------------------------------------------------------------------------------
.DS_Store
**/.DS_Store
._*

# Files that might appear on external disk
.Spotlight-V100
.Trashes
.com.apple.timemachine.supported


# Windows
# ----------------------------------------------------------------------------------------------
Thumbs.db


# Packages
# ----------------------------------------------------------------------------------------------
*.7z
*.dmg
*.gz
*.bz2
*.iso
*.jar
*.rar
*.tar
*.zip
*.tgz
*.tar.gz


# Logs and databases
# ----------------------------------------------------------------------------------------------
*.log
*.sql
*.htpasswd


# Sublime / Other Editors
# ----------------------------------------------------------------------------------------------
# cache files for sublime text
*.tmlanguage.cache
*.tmPreferences.cache
*.stTheme.cache
.idea
*.iml


# workspace files are user-specific
# ----------------------------------------------------------------------------------------------
*.sublime-workspace

# project files should be checked into the repository, unless a significant
# proportion of contributors will probably not be using SublimeText
*.sublime-project


# Tags
# ----------------------------------------------------------------------------------------------
# Ignore tags created by etags and ctags
TAGS
tags


# Vim
# ----------------------------------------------------------------------------------------------
#.*.sw[a-z]
#*.un~
Session.vim
